<Window x:Class="PMA_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PMA" Height="850" Width="1300" FontSize="12"
        xmlns:led="clr-namespace:LedDigital;assembly=LedDigital"
        Background="AliceBlue"
        xmlns:cvt="clr-namespace:PMA_Project.Other"
        WindowStartupLocation="CenterScreen"
        MinHeight="850" MinWidth="1300"        
        Closed="Window_Closed"        
        Icon=".\Images\icon.jpg"
        ResizeMode="NoResize"
        >
    <Window.Resources>
        <cvt:RadioButton_ZeroAsFalseConverter x:Key="ZeroAsFalseConverter"/>
        <cvt:RadioButton_ZeroAsTrueConverter x:Key="ZeroAsTrueConverter"/>
        <cvt:RadioButton_CC_IsoResStateConverter x:Key="IsoResStateConverter"/>
        <cvt:DV_CAN_RlyCtrlAllowConverter x:Key="RlyCtrlAllowConverter"/>
        <cvt:unit_mvToV_Converter x:Key="unit_mvToV"/>
        <Style TargetType="TextBlock">
            <Setter Property="Margin" Value="3,0"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="TextAlignment" Value="Right"/>
        </Style>
        <Style TargetType="GroupBox">
            <Setter Property="Padding" Value="2" />
            <Setter Property="Margin" Value="1"/>
        </Style>
        <Style TargetType="{x:Type RadioButton}">
            <Setter Property="Background" Value="Transparent"></Setter>
            <Setter Property="IsEnabled" Value="False"/>
            <Setter Property="Padding" Value="0"></Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RadioButton}">
                        <Grid x:Name="grid" Margin="{TemplateBinding Padding}" VerticalAlignment="Center">
                            <StackPanel Orientation="Horizontal">
                                <Image Margin="3,0,5,0" x:Name="icon" Width="16" Height="16" Source="/PMA_Project;component/images/circle_grey.png" />
                                <ContentPresenter VerticalAlignment="Center"/>
                            </StackPanel>
                        </Grid>
                        <!--触发器：设置选中状态符号-->
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsChecked" Value="true">
                                <Setter Property="Source" TargetName="icon"  Value="/PMA_Project;component/images/circle_green.png"/>
                                <Setter Property="Foreground" Value="Green"></Setter>
                            </Trigger>
                            <Trigger Property="IsMouseOver" Value="true">
                                <Setter Property="Foreground" Value="#007800"></Setter>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <!--<Setter Property="Opacity" Value="0.3" TargetName="grid" ></Setter>-->
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Grid Margin="3">
        <Grid.RowDefinitions >
            <RowDefinition  Height="*"/>
            <RowDefinition Height="90"/>
        </Grid.RowDefinitions>
        <StackPanel x:Name="BMSSts" Background="Transparent" VerticalAlignment="Top" HorizontalAlignment="Right" Orientation="Horizontal" Panel.ZIndex="1">
            <TextBlock Text="BMS通信状态："/>
            <TextBlock Text="{Binding ValDesc}" Width="40" FontSize="14"  Foreground="Orange"/>


        </StackPanel>
        <TabControl Grid.Row="0" Grid.Column="0" BorderThickness="1" >
            <TabItem Header="监控01">
                <Grid >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0" Grid.Column="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition />
                            <ColumnDefinition/>
                            <ColumnDefinition Width="1.3*"/>
                        </Grid.ColumnDefinitions>
                        <GroupBox Header="电池状态信息1">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2.2*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>
                                <GroupBox Header="Lin通信" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_Lin_State">
                                        <RadioButton Content="连接" GroupName="tab1_0" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_0" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="0" Grid.Column="1" Header="HVIL1状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_HVIL_State1" >
                                        <RadioButton Content="连接" GroupName="tab1_1" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_1" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="0" Grid.Column="2" Header="HVIL2状态">
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_HVIL_State2">
                                        <RadioButton Content="连接" GroupName="tab1_2" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_2" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="1" Grid.Column="0" Header="预充接触器状态"  >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_PrechgRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_3"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_3"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="1" Grid.Column="1" Header="总正接触器状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_PosRelayState" >
                                        <RadioButton Content="闭合" GroupName="tab1_4"    IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_4"     IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="1" Grid.Column="2" Header="总负接触器状态" >
                                    <StackPanel Orientation="Horizontal"  x:Name="CC_Ve_NegRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_5" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_5" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="2" Grid.Column="0" Header="慢充预充接触器状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_OBC_PrechgRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_6" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_6" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="2" Grid.Column="1" Header="慢充正极接触器状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_OBC_PosRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_7"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_7" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="2" Grid.Column="2" Header="慢充负极接触器状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_OBC_NegRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_8"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_8" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="3" Grid.Column="0" Header="快充预充接触器状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_FCH_PrechgRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_9"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_9" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="3" Grid.Column="1" Header="快充正极接触器状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_FCH_PosRelayState">
                                        <RadioButton Content="闭合" GroupName="tab1_10"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="断开" GroupName="tab1_10"   IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="3" Grid.Column="2" Header="碰撞状态" >
                                    <StackPanel Orientation="Horizontal"  x:Name="CC_Ve_CrashState">
                                        <RadioButton Content="碰撞" GroupName="tab1_11"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}"/>
                                        <RadioButton Content="未碰撞" GroupName="tab1_11" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox  Grid.Row="4" Grid.Column="0" Header="CC2信号状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_Ve_CC2State">
                                        <RadioButton Content="连接" GroupName="tab1_12" IsChecked="{Binding Data,Converter={StaticResource  ZeroAsFalseConverter}}" />
                                        <RadioButton Content="断开" GroupName="tab1_12"  IsChecked="{Binding Data,Converter={StaticResource  ZeroAsTrueConverter}}" />
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="4" Grid.Column="1" Header="单体电池总数" >
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  x:Name="TotalNum_Cell">
                                        <TextBlock Text="{Binding Data,StringFormat={}{0:n0}}" Foreground="#0070C0" FontWeight="Bold" FontSize="14" />
                                        <TextBlock Text="个"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="4" Grid.Column="2" Header="温度传感器总数" >
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  x:Name="TotalNum_TempSensor">
                                        <TextBlock Text="{Binding Data,StringFormat={}{0:n0}}" Foreground="#0070C0" FontWeight="Bold" FontSize="14" />
                                        <TextBlock Text="个"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="5" Grid.Column="1" Header="电池SOC" >
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  x:Name="INHW_Ve_HvBattSoc" >
                                        <TextBlock Text="{Binding Data,StringFormat={}{0:n2}}" Foreground="#0070C0" FontWeight="Bold" FontSize="14" />
                                        <TextBlock Text="%"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="5" Grid.Column="2" Header="电池平均温度" Grid.ColumnSpan="2" >
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  x:Name="INHW_Ve_AvgTemp" >
                                        <TextBlock Text="{Binding Data,StringFormat={}{0:n1}}" Foreground="#0070C0" FontWeight="Bold" FontSize="14" />
                                        <TextBlock Text="℃"/>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Grid.Row="5" Grid.Column="0" Header="绝缘状态" >
                                    <StackPanel Orientation="Horizontal" x:Name="CC_IsoRes_state" >
                                        <RadioButton Content="OK  " GroupName="tab1_13"  IsChecked="{Binding Data,Converter={StaticResource IsoResStateConverter }, ConverterParameter=0}"/>
                                        <RadioButton Content="Lv1  " GroupName="tab1_13" IsChecked="{Binding Data,Converter={StaticResource IsoResStateConverter }, ConverterParameter=1}" />
                                        <RadioButton Content="Lv2  " GroupName="tab1_13" IsChecked="{Binding Data,Converter={StaticResource IsoResStateConverter }, ConverterParameter=2}"/>
                                    </StackPanel>
                                </GroupBox>
                            </Grid>
                        </GroupBox>
                        <GroupBox Grid.Row="0" Grid.Column="1" Header="电池状态信息2 " >
                            <Grid Margin="3,0">

                                <Grid.RowDefinitions>
                                    <RowDefinition Height="58"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <StackPanel Orientation="Vertical" x:Name="INHW_Ve_IsoRes">
                                    <TextBlock Text="电池绝缘阻值（Kohm)" VerticalAlignment="Top" HorizontalAlignment="Left"/>

                                    <!--<led:DigitalPanelControl x:Name="ledTest1" Width="100" Height="60"  
                Value="{Binding LedDigitalValue1, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}"
                DigitalCount="3" 
                DigitalColor="Green"  >-->
                                    <Viewbox Width="auto" Height="24"  Margin="10,5"  >
                                        <led:DigitalPanelControl x:Name="ledTest1"  Height="60" Width="380"  Value="{Binding Data,StringFormat={}{0:n3}}"  DigitalCount="11" DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>
                                <StackPanel Grid.Row="1" x:Name="INHW_Ve_CoolantTemp_In" >
                                    <Border Grid.Row="1" BorderThickness="0,0.5,0,0" BorderBrush="#FFD5DFE5" Margin="2" DockPanel.Dock="Top"/>
                                    <TextBlock Text="进水口温度（℃)"  TextAlignment="Left"/>
                                    <Viewbox Width="auto" Height="24"  Margin="10,20"  >
                                        <led:DigitalPanelControl   Height="60" Width="180"   Value="{Binding Data,StringFormat={}{0:n1}}"   DigitalCount="5" DigitalColor="#006800" />
                                    </Viewbox>

                                </StackPanel>
                                <StackPanel Grid.Row="2" x:Name="INHW_Ve_CoolantTemp_Out">
                                    <TextBlock Text="出水口温度（℃)" DockPanel.Dock="Top" TextAlignment="Left"/>
                                    <Viewbox Width="auto" Height="24"  Margin="10,20">
                                        <led:DigitalPanelControl   Height="60" Width="180"   Value="{Binding Data,StringFormat={}{0:n1}}"   DigitalCount="5" DigitalColor="#006800" />
                                    </Viewbox>

                                </StackPanel>
                            </Grid>
                        </GroupBox>
                        <GroupBox Grid.Row="0" Grid.Column="2" Header="电池组状态">
                            <Grid>
                                <Grid.Resources>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Margin" Value="3,0"/>
                                        <Setter Property="VerticalAlignment" Value="Top"/>
                                        <Setter Property="Width" Value="140"/>
                                        <Setter Property="TextAlignment" Value="Right"/>
                                    </Style>
                                </Grid.Resources>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <StackPanel Grid.Row="0" Orientation="Horizontal" x:Name="INHW_Ve_CellVSumTotVolt">
                                    <TextBlock Text="电池电压累加和（V）" />
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="180"  Value="{Binding Data,StringFormat={}{0:n2}}"  DigitalCount="5" DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>
                                <StackPanel Grid.Row="1" Orientation="Horizontal" x:Name="INHW_Ve_DchBusVolt">
                                    <TextBlock Text="电池DCBus电压（V）" Width="140"/>
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="180" Value="{Binding Data,StringFormat={}{0:n2}}"  DigitalCount="5" DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>
                                <StackPanel Grid.Row="2" Orientation="Horizontal" x:Name="INHW_Ve_DchLinkVolt">
                                    <TextBlock Text="电池DCLink电压（V）" Width="140"/>
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="180" Value="{Binding Data,StringFormat={}{0:n2}}"   DigitalCount="5" Grid.Column="5" DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>
                                <StackPanel Grid.Row="3" Orientation="Horizontal" x:Name="INHW_Ve_ChargBusVolt">
                                    <TextBlock Text="电池ChargeBus电压（V）" Width="140"/>
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="180"  Value="{Binding Data,StringFormat={}{0:n2}}"  DigitalCount="5"  Grid.Column="5" DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>
                                <StackPanel Grid.Row="4" Orientation="Horizontal" x:Name="INHW_Ve_FuseVolt">
                                    <TextBlock Text="电池Fuse电压（V）" Width="140" />
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="180"  Value="{Binding Data,StringFormat={}{0:n2}}"  DigitalCount="5"  Grid.Column="5" DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>
                            </Grid>
                        </GroupBox>
                        <GroupBox Grid.Row="0" Grid.Column="3" Header="BMS工作模式">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Row="0" Grid.Column="0" Text="系统模式"/>
                                <TextBlock Grid.Row="1" Grid.Column="0" Text="行车模式状态"/>
                                <TextBlock Grid.Row="2" Grid.Column="0" Text="直流快充请求状态"/>
                                <TextBlock Grid.Row="3" Grid.Column="0" Text="交流充电模式状态"/>
                                <TextBlock Grid.Row="4" Grid.Column="0" Text="直流快充模式状态"/>
                                <TextBlock Grid.Row="5" Grid.Column="0" Text="直流快充接触器控制模式"/>
                                <TextBlock Grid.Row="6" Grid.Column="0" Text="行车模式接触器控制模式"/>
                                <TextBlock Grid.Row="7" Grid.Column="0" Text="热管理模式状态"/>
                                <TextBlock Grid.Row="8" Grid.Column="0" Text="交流充电接触器控制模式"/>
                                <StackPanel  Grid.Row="0" Grid.Column="1" VerticalAlignment="Center" x:Name="mWMD_SysMod_U08">
                                    <TextBlock TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel  Grid.Row="1" Grid.Column="1" VerticalAlignment="Center" x:Name="mWMD_DrvModSts_U08" >
                                    <TextBlock  TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel  Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" x:Name="mCHG_DcChgReqMod_U08">
                                    <TextBlock TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel  Grid.Row="3" Grid.Column="1" VerticalAlignment="Center" x:Name="mCHG_AcChgModSts_U08">
                                    <TextBlock  TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel  Grid.Row="4" Grid.Column="1" VerticalAlignment="Center" x:Name="mCHG_DcChgModSts_U08">
                                    <TextBlock  TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel   Grid.Row="5" Grid.Column="1" VerticalAlignment="Center" x:Name="mRLY_DcChgRlyCtrlMod_U08">
                                    <TextBlock TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel  Grid.Row="6" Grid.Column="1" VerticalAlignment="Center" x:Name="mRLY_DrvRlyCtrlMod_U08">
                                    <TextBlock  TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel Grid.Row="7" Grid.Column="1" VerticalAlignment="Center" x:Name="mTHM_ModSts_U08">
                                    <TextBlock  TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                                <StackPanel  Grid.Row="8" Grid.Column="1"  VerticalAlignment="Center" x:Name="mRLY_AcChgRlyCtrlMod_U08">
                                    <TextBlock TextAlignment="Left" FontWeight="Bold" FontSize="14" Foreground="#0070C0" Text="{Binding Data}"/>
                                </StackPanel>
                            </Grid>
                        </GroupBox>
                    </Grid>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="243*" />
                            <ColumnDefinition Width="451*" />
                            <ColumnDefinition Width="201*" />
                            <ColumnDefinition Width="371*"/>
                        </Grid.ColumnDefinitions>
                        <GroupBox Grid.Row="0" Grid.Column="0" Header="电池温度状态">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="26"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="26"/>
                                    <RowDefinition Height="26"/>
                                </Grid.RowDefinitions>

                                <Canvas Grid.Row="1" x:Name="canvasMaxTemp" >

                                </Canvas>
                                <StackPanel Grid.Row="2" Orientation="Horizontal"   x:Name="INHW_Ve_CellMaxTemp">
                                    <TextBlock Text="最高温度"  />
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl  Height="60" Width="150"  Value="{Binding Data,StringFormat={}{0:n1}}"  DigitalCount="5"   Grid.Column="5" DigitalColor="#006800"/>
                                    </Viewbox>
                                </StackPanel>

                                <Canvas x:Name="canvasMinTemp" Grid.Row="1" Grid.Column="1" >

                                </Canvas>

                                <StackPanel Grid.Row="2" Grid.Column="1" Orientation="Horizontal" x:Name="INHW_Ve_CellMinTemp">
                                    <TextBlock Text="最低温度" />
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="150"  Value="{Binding Data,StringFormat={}{0:n1}}"  DigitalCount="5"  Grid.Column="5" DigitalColor="#006800"  />
                                    </Viewbox>
                                </StackPanel>
                                <StackPanel Grid.Row="3" Orientation="Horizontal" x:Name="INHW_Ve_CellMaxTempNo">
                                    <TextBlock Text="温度编号" />
                                    <Viewbox Width="auto" Height="24" >
                                        <led:DigitalPanelControl   Height="60" Width="150"  Value="{Binding Data,StringFormat={}{0:n0}}"  DigitalCount="5"  DigitalColor="#006800" />
                                    </Viewbox>

                                </StackPanel>
                                <StackPanel Grid.Row="3" Grid.Column="1" Orientation="Horizontal" x:Name="INHW_Ve_CellMinTempNo">
                                    <TextBlock Text="温度编号" />
                                    <Viewbox Width="auto" Height="24"  >
                                        <led:DigitalPanelControl   Height="60" Width="150"  Value="{Binding Data,StringFormat={}{0:n0}}"  DigitalCount="5"   DigitalColor="#006800" />
                                    </Viewbox>
                                </StackPanel>

                                <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Orientation="Horizontal" HorizontalAlignment="Center" x:Name="INHW_Ve_TempDiffMax">
                                    <TextBlock Text="电池温差" />
                                    <Viewbox Width="auto" Height="24"  Margin="10,0">
                                        <led:DigitalPanelControl   Height="60" Width="180"  Value="{Binding Data,StringFormat={}{0:n1}}"  DigitalCount="5" DigitalColor="#006800" />
                                    </Viewbox>
                                    <TextBlock Text="(℃)"/>
                                </StackPanel>
                            </Grid>
                        </GroupBox>
                        <Grid Grid.Column="2">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="7*"/>
                                <RowDefinition Height="2*"/>
                            </Grid.RowDefinitions>
                            <GroupBox Header="唤醒源状态">
                                <Grid>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Border BorderThickness="1" BorderBrush="#FFD5DFE5" CornerRadius="2" Padding="2">
