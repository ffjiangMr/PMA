        <Grid Grid.Row="1" Panel.ZIndex="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="756"/>
                <ColumnDefinition Width="509*" />

            </Grid.ColumnDefinitions>
            <GroupBox Grid.Column="0" Header="数据存储">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>

                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="文件夹" Width="60" TextAlignment="Right"/>
                        <TextBox  Width="342" Margin="1" BorderBrush="#FFC7D2DD" VerticalAlignment="Center"   Height="26" IsReadOnly="True" Text="{Binding FilePath}" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                        <Button Content="选择" x:Name="btnChoose"  Margin="15,1" Width="50" Background="#FDB603" BorderBrush="#FFD49A08" Click="Button_Click" IsEnabled="{Binding ElementName=btnStartRecord,Path=IsEnabled}"  />
                        <TextBlock Text="文件名" Margin=" 50,0,3,0"/>
                        <TextBox Width="100" Margin="1" BorderBrush="#FFC7D2DD" VerticalAlignment="Center"   Height="26" Text="{Binding FileName}" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsEnabled="{Binding ElementName=btnStartRecord,Path=IsEnabled}" />
                    </StackPanel>
                    <StackPanel Grid.Row="1" Orientation="Horizontal">
                        <TextBlock Text="记录行数" Width="60"  TextAlignment="Right"/>
                        <TextBox Width="100" Margin="1" BorderBrush="#FFC7D2DD" VerticalAlignment="Center"   Height="26" Text="{Binding LineLimit}" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsEnabled="{Binding ElementName=btnStartRecord,Path=IsEnabled}" />
                        <TextBlock Text="周期(ms)" Width="134" TextAlignment="Right" />
                        <TextBox Width="100" Margin="1" BorderBrush="#FFC7D2DD" VerticalAlignment="Center"   Height="26" Text="{Binding Interval}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Center" IsEnabled="{Binding ElementName=btnStartRecord,Path=IsEnabled}" />
                        <Button Content="应用" x:Name="btnApply" Margin="15,1" Width="50" Background="#FDB603" BorderBrush="#FFD49A08" Click="Button_Click_1" IsEnabled="{Binding ElementName=btnStartRecord,Path=IsEnabled}" />
                        <Button Content="开始保存" x:Name="btnStartRecord" Margin="50,1,15,1" Width="80" Background="#FDB603" BorderBrush="#FFD49A08" Click="StartRecord"/>
                        <Button Content="停止保存" x:Name="btnStopRecord" Margin="15,1" Width="80" Background="#FDB603" BorderBrush="#FFD49A08" Click="StopRecord" />
                    </StackPanel>

                </Grid>

            </GroupBox>
            <GroupBox Grid.Column="1" Header="系统配置">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <StackPanel Margin="50,0">
                        <StackPanel.Resources>
                            <Style x:Key="style_btnStart" TargetType="{x:Type Button}">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <StackPanel Orientation="Horizontal" >
                                                <Image Name="img" Source="pack://application:,,,/images/play01.png" />
                                            </StackPanel>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter Property="Source" Value="pack://application:,,,/images/play02.png" TargetName="img"/>
                                                </Trigger>
                                                <Trigger Property="IsEnabled" Value="False">
                                                    <Setter Property="Source" Value="pack://application:,,,/images/play03.png" TargetName="img"/>
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>

                        </StackPanel.Resources>
                        <Button x:Name="btnSysCtrlStart" Width="48" Height="48" Style="{StaticResource style_btnStart}" Click="btnSysCtrlStart_Click"/>

                        <TextBlock Text="启动" TextAlignment="Center"/>
                    </StackPanel>
                    <StackPanel>
                        <StackPanel.Resources>
                            <Style x:Key="style_btnStop" TargetType="{x:Type Button}">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <StackPanel Orientation="Horizontal" >
                                                <Image Name="imgStop" Source="pack://application:,,,/images/stop01.png" />
                                            </StackPanel>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter Property="Source" Value="pack://application:,,,/images/stop02.png" TargetName="imgStop"/>
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>

                        </StackPanel.Resources>
                        <Button x:Name="btnSysCtrlStop" Width="48" Height="48" Style="{StaticResource style_btnStop}" Click="btnSysCtrlStop_Click"/>

                        <TextBlock Text="停止" TextAlignment="Center"/>
                    </StackPanel>
                    <StackPanel  Margin="50,0">
                        <StackPanel.Resources>
                            <Style x:Key="style_btnSetting" TargetType="{x:Type Button}">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="{x:Type Button}">
                                            <StackPanel Orientation="Horizontal" >
                                                <Image Name="ImgBtnBg3" Source="pack://application:,,,/images/settings01.png" />
                                            </StackPanel>
                                            <ControlTemplate.Triggers>
                                                <Trigger Property="IsMouseOver" Value="True">
                                                    <Setter Property="Source" Value="pack://application:,,,/images/settings02.png" TargetName="ImgBtnBg3"/>
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </StackPanel.Resources>
                        <Button x:Name="btnSettings" Width="48" Height="48" Style="{StaticResource style_btnSetting}" Click="btnSettings_Click"/>
                        <TextBlock Text="配置" TextAlignment="Center"/>
                    </StackPanel>





                </StackPanel>
            </GroupBox>
        </Grid>
    </Grid>
</Window>
